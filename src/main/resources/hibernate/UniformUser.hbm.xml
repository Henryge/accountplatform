<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping package="com.uniform.accountplatform.model">

	<class name="UniformUser" table="uniform_user">
		<id name="id" column="id">
			<generator class="identity"/>
		</id>
		<property name="userId" column="user_id"/>
		<property name="userName" column="user_name"/>
		<property name="userCellPhone" column="user_cellphone"/>
		<property name="userEmail" column="user_email"/>
		<property name="userStatus" column="user_status"/>
	</class>
	
</hibernate-mapping>